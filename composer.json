{
    "name": "mrkatz/input",
    "description": "html, input generation",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Kaczocha",
            "email": "adamkaczocha@gmail.com",
            "homepage": "https://github.com/mrkatz/input"
        }
    ],
    "homepage": "https://github.com/mrkatz/input",
    "keywords": ["Laravel", "input"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Mrkatz\\Input\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mrkatz\\Input\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrkatz\\Input\\InputServiceProvider"
            ],
            "aliases": {
                "Input": "Mrkatz\\Input\\Facades\\Input"
            }
        }
    }
}
